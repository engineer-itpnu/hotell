{% extends '::base.html.twig' %}

{% block body -%}
    <h1>roomEntity</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Room_type</th>
                <td>{{ entity.roomtype }}</td>
            </tr>
            <tr>
                <th>Room_name</th>
                <td>{{ entity.roomname }}</td>
            </tr>
            <tr>
                <th>Room_capacity</th>
                <td>{{ entity.roomcapacity }}</td>
            </tr>
            <tr>
                <th>Room_addcapacity</th>
                <td>{{ entity.roomaddCapacity }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('roomm') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('roomm_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
