{% extends '::base.html.twig' %}

{% block body -%}
    <h1>customerEntity</h1>

    <table class="record_properties">
        <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Cust_name</th>
            <td>{{ entity.custname }}</td>
        </tr>
        <tr>
            <th>Cust_family</th>
            <td>{{ entity.custfamily }}</td>
        </tr>
        <tr>
            <th>Cust_email</th>
            <td>{{ entity.custemail }}</td>
        </tr>
        <tr>
            <th>Cust_phone</th>
            <td>{{ entity.custphone }}</td>
        </tr>
        <tr>
            <th>Cust_mobile</th>
            <td>{{ entity.custmobile }}</td>
        </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('customerentity') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('customerentity_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
