{% extends '::base.html.twig' %}

{% block body -%}
    <h1>masterEntity</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Accountstock</th>
                <td>{{ entity.accountStock }}</td>
            </tr>
            <tr>
                <th>Accountchangevalue</th>
                <td>{{ entity.accountChangeValue }}</td>
            </tr>
            <tr>
                <th>Accountstatus</th>
                <td>{{ entity.accountStatus }}</td>
            </tr>
            <tr>
                <th>Accountreqestdata</th>
                <td>{{ entity.accountReqestData }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('master') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('master_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
