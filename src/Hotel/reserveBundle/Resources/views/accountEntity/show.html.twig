{% extends '::base.html.twig' %}

{% block body -%}
    <h1>accountEntity</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Price</th>
                <td>{{ entity.price }}</td>
            </tr>
            <tr>
                <th>Type</th>
                <td>{{ entity.type }}</td>
            </tr>
            <tr>
                <th>Stockhotel</th>
                <td>{{ entity.StockHotel }}</td>
            </tr>
            <tr>
                <th>Stockagency</th>
                <td>{{ entity.StockAgency }}</td>
            </tr>
            <tr>
                <th>Numberpey</th>
                <td>{{ entity.NumberPey }}</td>
            </tr>
            <tr>
                <th>Datetime</th>
                <td>{{ entity.DateTime|date('Y-m-d H:i:s') }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('accountentity') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('accountentity_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
